PROGRAM p
BEGIN

	INT a,b,k,g,p,u;

	FUNCTION VOID main()
	BEGIN

		a := 1;
		b := 1;
          DO
			IF (b = 1)
				g := 1*a;
				WRITE (g);
			ELSIF (b = 2)
				p := 2*a;
				WRITE (p);
			ELSIF (b = 3)
				k := 3*a;
				WRITE (k);
			ELSIF (b = 4)
				u := 4*a;
				WRITE (u);
			ENDIF
			b := b + 1;
			a := a + 20;
		WHILE (a <= 120);

	END
END
